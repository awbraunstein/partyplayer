<li class='search-result' data-uri='{{uri}}' data-source='{{source}}'
 data-title='{{title}}' data-artist='{{artist}}'>
  <img class='album-art' src='http://placehold.it/60x60' />
  <div class='meta'>
    <p class='track-title'>{{title}}</p>
    {{#if artist}}
    <p class='track-artist'>{{artist}}</p>
    {{/if}}
  </div>
</li>
